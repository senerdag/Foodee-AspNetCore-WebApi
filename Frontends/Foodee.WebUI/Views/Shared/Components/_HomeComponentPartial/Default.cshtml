@model List<ResultBannerDto>

<div id="fh5co-home" class="js-fullheight" data-section="home">

	<div class="flexslider">
		@foreach (var item in Model)
		{
		<div class="fh5co-overlay"></div>
		<div class="fh5co-text">
			
				<div class="container">
					<div class="row">
						<h1 class="to-animate" style="color:coral">@item.Title</h1>
						<h2 class="to-animate">@item.Description</h2>
					</div>
				</div>
			
			
		</div>
		<ul class="slides">
			<li style="background-image: url(@item.CoverImageUrl);" data-stellar-background-ratio="0.5"></li>
		</ul>
		}
	</div>

</div>